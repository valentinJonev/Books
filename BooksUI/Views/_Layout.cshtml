<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    
    @Styles.Render("~/bundles/bootstrapCSS")
    @Styles.Render("~/bundles/dataTablesCSS")
</head>
<body>
    @{Html.RenderPartial("_Navbar");}
    <div>
        @RenderBody()
